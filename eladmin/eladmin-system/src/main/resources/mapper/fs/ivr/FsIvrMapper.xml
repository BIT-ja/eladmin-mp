<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="me.zhengjie.fs.ivr.mapper.FsIvrMapper">
    <resultMap id="BaseResultMap" type="me.zhengjie.fs.ivr.domain.FsIvr">
        <id column="id" property="id"/>
        <result column="dest_num" property="destNum"/>
        <result column="tag" property="tag"/>
        <result column="create_time" property="createTime"/>
        <result column="create_by" property="createBy"/>
        <result column="update_time" property="updateTime"/>
        <result column="update_by" property="updateBy"/>
    </resultMap>

    <sql id="Base_Column_List">
        id, dest_num, tag, create_time, create_by, update_time, update_by
    </sql>

    <select id="findAll" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from fs_ivr
        <where>
            <if test="criteria.id != null">
                and id = #{criteria.id}
            </if>
            <if test="criteria.destNum != null">
                and dest_num = #{criteria.destNum}
            </if>
            <if test="criteria.tag != null">
                and tag = #{criteria.tag}
            </if>
            <if test="criteria.createBy != null">
                and create_by = #{criteria.createBy}
            </if>
            <if test="criteria.updateBy != null">
                and update_by = #{criteria.updateBy}
            </if>
            <if test="criteria.createTime != null and criteria.createTime.size() > 0">
                AND create_time BETWEEN #{criteria.createTime[0]} AND #{criteria.createTime[1]}
            </if>
            <if test="criteria.updateTime != null and criteria.updateTime.size() > 0">
                AND update_time BETWEEN #{criteria.updateTime[0]} AND #{criteria.updateTime[1]}
            </if>
        </where>
        order by id desc
    </select>
</mapper>